*******************************************************************************
* SPICE Model Library
*******************************************************************************
@# To instantiate a new model library, specify the following fields and run:
@# unix> em.py model_tmpl.lib > models.lib 
@{
process = "ptm065"
corners = ["TT", "FF", "SS", "FS", "SF"]
nmos_typ = "65nm_NMOS_bulk_tt.pm"
nmos_fast = "65nm_NMOS_bulk_ff.pm"
nmos_slow = "65nm_NMOS_bulk_ss.pm"
pmos_typ = "65nm_PMOS_bulk_tt.pm"
pmos_fast = "65nm_PMOS_bulk_ff.pm"
pmos_slow = "65nm_PMOS_bulk_ss.pm"
}@
* Process technology: @process
*   NMOS models
*       typical : @nmos_typ
*       fast    : @nmos_fast
*       slow    : @nmos_slow
*   PMOS models
*       typical : @pmos_typ
*       fast    : @pmos_fast
*       slow    : @pmos_slow
*******************************************************************************

@{
nmos_model = dict(T=nmos_typ, F=nmos_fast, S=nmos_slow)
pmos_model = dict(T=pmos_typ, F=pmos_fast, S=pmos_slow)

for corner in corners:
    empy.string(".LIB %s\n" % corner)
    empy.string(".include '%s'\n" % nmos_model[corner[0]])
    empy.string(".include '%s'\n" % pmos_model[corner[1]])
    empy.string(".ENDL\n\n")
}@
